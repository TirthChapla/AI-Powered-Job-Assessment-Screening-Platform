{
  "name": "@voicehire/faq-plugin",
  "version": "1.0.0",
  "description": "VoiceHire FAQ Voice Agent Plugin - A TypeScript library for integrating voice-based FAQ assistance",
  "main": "dist/vh-faq-plugin.umd.js",
  "module": "dist/vh-faq-plugin.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "dev:build": "tsc && vite build --watch --mode development",
    "build": "tsc && vite build --mode production",
    "build:dev": "tsc && vite build --mode development",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "voicehire",
    "faq",
    "voice",
    "agent",
    "typescript",
    "plugin"
  ],
  "author": "VoiceHire Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/voicehire/voice-hire"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.29",
    "@vitejs/plugin-legacy": "^6.1.1",
    "rimraf": "^6.0.1",
    "terser": "^5.40.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "livekit-client": "^2.13.3"
  }
}
